{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": 0,
    "no-empty": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "quotes": [1, "single"],
    "no-multi-spaces": 1,
    "key-spacing": [0, {"align": "value"}],
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0
  }
}
